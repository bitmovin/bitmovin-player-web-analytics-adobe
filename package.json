{
  "name": "bitmovin-player-analytics-adobe",
  "version": "1.0.0-beta.4",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "serve": "http-server",
    "wp-server": "webpack-dev-server --content-base public/",
    "start": "concurrently --kill-others \"webpack --watch -d\" \"npm run wp-server\"",
    "docs": "typedoc --out docs/ --target ES6 --mode modules --module commonjs src/bitmovin-heartbeat.ts ",
    "format": "prettier --single-quote --parser typescript --write 'src/**/*.{ts,js}' 'test/**/*.{ts,js}'",
    "precommit": "pretty-quick --staged",
    "prepare-release": "webpack --mode='production' --devtool='-' && cp public/js/bitmovinplayer-adobeanalytics.js dist/ && npm run docs && cd dist && npm pack"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "bitmovin-player": "^8.31.0",
    "bitmovin-player-ui": "^3.10.0",
    "concurrently": "^5.1.0",
    "husky": "^4.2.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1",
    "ts-loader": "^4.5.0",
    "typescript": "^2.9.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "dts-gen": "^0.6.0",
    "typedoc": "^0.16.10"
  }
}
